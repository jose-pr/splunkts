    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Stream Event Test",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "preLaunchTask": "build",
            "cwd": "${workspaceFolder}\\packages\\modularinputs",
            "program": "${workspaceFolder}\\packages\\modularinputs\\lib\\index.js",
            "args": [
                "streamevents",
                "test/inputdefinition.xml",
                "out/out.xml",
                "out/error.txt"
                
            ],
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
    ]
}